	</insert>
	
	<!-- ===========================3、修改===============================  -->

	<!-- 油品入库 -->
	<!-- Integer updateStock(String ramount,String oid); -->
	
	<update id="updateStock" parameterType="java.lang.Integer">
		UPDATE 
			IOT_EQUIPMENT_LUBRICATION_OIL
				<set>
				   ostock = ostock+#{ramount}
				</set>
				<where>
					oid = #{oid}
				</where>
	</update>
	
	<!-- ===========================4、删除===============================  -->
			
	
</mapper>
 

