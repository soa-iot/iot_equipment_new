					#{eqor.rid},
				</if>
				<if test="eqor.rinout != null and eqor.rinout != ''">
					#{eqor.rinout},
				</if>
				<if test="eqor.ramount != null and eqor.ramount != ''">
					#{eqor.ramount},
				</if>
				<if test="eqor.userid != null and eqor.userid != ''">
